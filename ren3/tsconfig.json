{
  "compilerOptions": {
    "declaration": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "lib": [
      "DOM",
      "ES5",
      "ES2015.Collection",
      "ES2015.Promise"
    ],
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist",
    "strict": true
  },
  "include": [
    "./src/**/*.ts"
  ]
}
